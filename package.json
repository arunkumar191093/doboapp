{
  "name": "doboapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "bundle-android-apk": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle-release-android-apk": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/*",
    "curl-android": "curl 'http://localhost:8081/index.bundle?platform=android' -o 'android/app/src/main/assets/index.android.bundle'",
    "create-apk": "cd android && ./gradlew clean assembleDebug && cd ..",
    "create-release-apk": "cd android && ./gradlew assembleRelease && cd ..",
    "create-release-aab": "cd android && ./gradlew bundleRelease && cd .."
  },
